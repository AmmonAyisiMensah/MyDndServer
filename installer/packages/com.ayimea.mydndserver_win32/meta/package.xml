<?xml version="1.0" encoding="UTF-8"?>
<Package>
    <DisplayName>Ayimea My DnD Server</DisplayName>
    <Description>Simulate a D&D game in a web application that can handle user with a character from DndBeyond.</Description>
    <Version>1.0.0</Version>
    <ReleaseDate>2023-10-08</ReleaseDate>
    <Licenses>
        <License name="Ayimea Personal use License Agreement" file="license.txt" />
    </Licenses>
    <Default>true</Default>
	<Operations>
	  <Operation name="CreateShortcut">
		<Argument>@TargetDir@/MyDndServer.exe</Argument>
		<Argument>@DesktopDir@/MyDndServer.lnk</Argument>
	  </Operation>
	  <Operation name="CreateShortcut">
		<Argument>@TargetDir@/MyDndServer.exe</Argument>
		<Argument>@AllUsersStartMenuProgramsPath@/MyDndServer.lnk</Argument>
	  </Operation>
	</Operations>
    <Script></Script>
    <UserInterfaces></UserInterfaces>
</Package>
